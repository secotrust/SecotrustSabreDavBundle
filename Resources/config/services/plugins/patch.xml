<?xml version="1.0" ?>

<container xmlns="http://symfony.com/schema/dic/services"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://symfony.com/schema/dic/services http://symfony.com/schema/dic/services/services-1.0.xsd">

    <parameters>
        <parameter key="secotrust.sabredav.patch_plugin.class">Sabre\DAV\PartialUpdate\Plugin</parameter>
    </parameters>

    <services>
        <service id="secotrust.sabredav.patch_plugin" class="%secotrust.sabredav.patch_plugin.class%" public="false">
            <tag name="secotrust.sabredav.plugin" />
        </service>
    </services>
</container>
